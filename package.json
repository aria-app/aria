    "react-redux": "7.1.1",
    "react-render-helpers": "^1.2.0",
    "react-router-dom": "^5.0.1",
    "redux": "4.0.4",
    "redux-observable": "1.2.0",
    "redux-starter-kit": "0.9.1",
    "redux-undo": "^1.0.0-beta9-9-7",
    "rxjs": "6.5.3",
    "shortid": "^2.2.14",
    "tone": "^13.4.9",
    "snyk": "^1.316.1"
