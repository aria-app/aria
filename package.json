    "@reach/router": "1.2.1",
    "classnames": "^2.2.6",
    "dawww": "^1.10.2",
    "deep-diff": "^1.0.2",
    "event-emitter": "^0.3.5",
    "firebase": "^6.3.0",
    "framer-motion": "^1.6.8",
    "i18next": "^17.0.4",
    "i18next-browser-languagedetector": "^3.0.1",
    "lodash": "^4.17.12",
    "memoize-one": "^5.0.5",
    "polished": "^3.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-draggable": "^4.0.3",
    "react-hotkeys": "^2.0.0",
