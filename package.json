    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "eslint-config-react-app": "^5.0.2",
    "husky": "3.0.9",
    "prettier": "^1.18.2",
    "react-scripts": "^3.1.2",
    "typescript": "^3.6.3"
  },
  "eslintConfig": {
    "extends": "react-app"
