{
  "name": "aria",
  "version": "1.0.0",
  "description": "Easy-to-use music sequencer inspired by Little Big Planet 2 + 3. ",
  "homepage": "https://ariaapp.io",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "clean": "rimraf src/**/*.js",
    "build-storybook": "build-storybook -s public",
    "eslint": "eslint \"src/**\" --fix --ignore-pattern \"src/**/*.json\"",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "yarn run eslint && yarn run prettier && yarn run tsc --noEmit",
    "prestart": "yarn run clean",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "tsc": "tsc",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "dependencies": {
    "@loadable/component": "5.10.3",
